package com.ss.Array;

public class ReversePosition {

	public static void main(String[] args) {
		
		int arr[]= {10,20,30,40,50,60};
		
		int temp[]=new int[arr.length];
		
		int count=0;
		
		for(int i=arr.length-1;i>=0;i--) {
			
			temp[count]=arr[i];
			count++;
		}
		
		for(int a:temp) {
			System.out.print(a+" ");
		}

	}

}
