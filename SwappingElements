package com.ss.Array;

public class SwappingElements {

	public static void main(String[] args) {
		
		int []arr= {10,20,30,40,50,60,70};
		
		int temp=0;
		
		for(int i=0;i<=arr.length/2;i++) {
			temp=arr[i];
			arr[i]=arr[arr.length-1-i];
			arr[arr.length-1-i]=temp;
		}
		
		for(int a:arr) {
			System.out.print("=>"+a);
		}

	}

}
