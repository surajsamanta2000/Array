package com.ss.Array;

public class MinMax {

	public static void main(String[] args) {
		int arr[]={40,70,65,26,97,12};
		
		int max=arr[0];
		int min=arr[0];
		
		for(int i=1;i<arr.length;i++) {
			if(arr[i]>max) {
				max=arr[i];
			}
			
			if(arr[i]<min) {
				min=arr[i];
			}
		}
		
		System.out.println("Min Element ::"+min);
		System.out.println("Max Element ::"+max);
	}

}
